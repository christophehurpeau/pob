{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@pob:target:BaseBundler",

  "compilerOptions": {
    "importHelpers": true,
    "module": "node18",
    "moduleResolution": "node16",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true
  }
}
