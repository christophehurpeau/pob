{
  "name": "@pob/lerna-light",
  "version": "6.0.0",
  "description": "import only required commands from lerna",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pob.git",
    "directory": "@pob/lerna-light"
  },
  "homepage": "https://github.com/christophehurpeau/pob",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.cjs",
  "exports": {
    ".": "./lib/index.cjs",
    "./bin/cli.js": "./bin/cli.cjs",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "bin": {
    "lerna": "bin/cli.cjs"
  },
  "scripts": {
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet @pob/lerna-light"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {},
  "dependencies": {
    "@lerna/cli": "5.5.4",
    "@lerna/publish": "5.5.4",
    "@lerna/version": "5.5.4"
  }
}
