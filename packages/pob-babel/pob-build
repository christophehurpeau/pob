#!/usr/bin/env bash

rm -f ./dist/*

if [ -f ./node_modules/pob-babel/node_modules/rollup/bin/rollup ]; then
  ./node_modules/pob-babel/node_modules/rollup/bin/rollup --config ./node_modules/pob-babel/rollup.config.js
else
  ./node_modules/rollup/bin/rollup --config ./node_modules/pob-babel/rollup.config.js
fi
