{
    "env": {
        "doc": {
            "presets": ["es2015", "flow", "stage-1"],
            "plugins": ["add-jsdoc-annotations"]
        },
        "es5": {
            "presets": ["es2015", "flow", "stage-1"]
        },
        "es5-dev": {
            "presets": ["es2015", "flow", "stage-1"],
            "plugins": ["typecheck"]
        },
        "node5": {
            "presets": ["es2015-node5", "flow", "stage-1"]
        },
        "node5-dev": {
            "presets": ["es2015-node5", "flow", "stage-1"],
            "plugins": ["typecheck"]
        },
        "node6": {
            "presets": ["es2015-node6/object-rest", "flow", "stage-1"]
        },
        "node6-dev": {
            "presets": ["es2015-node6/object-rest", "flow", "stage-1"],
            "plugins": ["typecheck"]
        },
        "webpack": {
            "presets": ["es2015-webpack", "flow", "stage-1"]
        },
        "webpack-dev": {
            "presets": ["es2015-webpack", "flow", "stage-1"],
            "plugins": ["typecheck"]
        },
        "webpack-modern-browsers": {
            "presets": ["modern-browsers/webpack2", "flow", "modern-browsers/stage1"]
        },
        "webpack-modern-browsers-dev": {
            "presets": ["modern-browsers/webpack2", "flow", "modern-browsers/stage1"],
            "plugins": ["typecheck"]
        }
    }
}
