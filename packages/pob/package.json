    "@pob/eslint-config-typescript-react": "51.0.0",
    "@pob/sort-eslint-config": "5.0.0",
    "@pob/sort-object": "6.0.0",
    "@pob/sort-pkg": "6.0.0",
    "eslint": "8.41.0",
    "findup-sync": "^5.0.0",
