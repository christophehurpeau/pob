{
  "name": "pob",
  "version": "23.4.1",
  "description": "Pile of bones, library generator with git/babel/typescript/typedoc/readme/jest",
  "keywords": [
    "skeleton"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pob.git",
    "directory": "packages/pob"
  },
  "homepage": "https://github.com/christophehurpeau/pob",
  "bugs": {
    "url": "https://github.com/christophehurpeau/pob/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "sideEffects": false,
  "bin": "./lib/pob.js",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "format": "prettier --write",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --quiet packages/pob"
  },
  "pob": {
    "typescript": "check-only"
  },
  "prettier": "@pob/root/prettier-config",
  "dependencies": {
    "@pob/eslint-config": "58.1.0",
    "@pob/eslint-config-typescript": "58.1.0",
    "@pob/eslint-config-typescript-react": "58.1.0",
    "@pob/sort-eslint-config": "6.0.0",
    "@pob/sort-object": "7.0.0",
    "@pob/sort-pkg": "8.0.0",
    "@prettier/sync": "0.5.2",
    "@types/inquirer": "9.0.7",
    "@yarnpkg/cli": "4.5.1",
    "@yarnpkg/core": "4.1.4",
    "@yarnpkg/fslib": "3.1.0",
    "@yeoman/types": "1.3.0",
    "eslint": "9.13.0",
    "findup-sync": "^5.0.0",
    "git-remote-url": "^1.0.1",
    "github-username": "^7.0.0",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "mem-fs": "4.1.1",
    "mem-fs-editor": "11.1.1",
    "minimist": "1.2.8",
    "parse-author": "2.0.0",
    "pob-dependencies": "workspace:*",
    "prettier": "3.3.3",
    "semver": "7.6.3",
    "validate-npm-package-name": "^6.0.0",
    "yarn-workspace-utils": "6.1.0",
    "yeoman-environment": "4.4.2",
    "yeoman-generator": "7.3.2"
  },
  "devDependencies": {
    "@pob/root": "13.4.1",
    "@types/node": "20.16.13",
    "typescript": "5.6.3"
  }
}
