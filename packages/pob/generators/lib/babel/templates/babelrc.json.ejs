{
<% if (hasReact) { -%>
  "presets": [
    "pob-react"
  ],
<% } -%>
  "env": {
<% if (testing) { -%>
    "test": {
      "only": ["src/"],
      "presets": [
        ["pob-babel/preset", { "flow": <%= hasFlow ? 'true' : 'false' %> }]
      ]
    }<% if (documentation) { %>,<% } %>
<% } if (documentation) { -%>
    "doc": {
      "only": ["src/"],
      "presets": [
        ["pob-babel/jsdoc-preset", { "flow": <%= hasFlow ? 'true' : 'false' %> }]
      ]
    }
<% } -%>
  }
}
