/* eslint-disable import/no-dynamic-require, prettier/prettier */

'use strict';

const production = process.env.NODE_ENV === 'production';
<% var envs = [
    ['node10', node10],
].filter(v => v[1]);
if (envs.length === 1) { -%>
module.exports = require(`./dist/<%= entry %>-<%= envs[0][0] %>${production ? '' : '-dev'}.cjs`);
<% } else { -%>
<%# parseFloat(process.versions.node) doesnt work with 6.10.1 -%>
const nodeVersion = process.versions.node.split('.');
const nodeVersionMajor = Number(nodeVersion[0]);
const nodeVersionMinor = Number(nodeVersion[1]);
/* istanbul ignore next */
if (nodeVersionMajor > 10 || (nodeVersionMajor === 10 && nodeVersionMinor >= 13)) {
  module.exports = require(`./dist/<%= entry %>-node10${production ? '' : '-dev'}.cjs`);
/* istanbul ignore next */
} else {
  throw new Error('Node version not supported: ' + nodeVersion + ' (' + process.versions.node + ').');
}
<% } -%>
