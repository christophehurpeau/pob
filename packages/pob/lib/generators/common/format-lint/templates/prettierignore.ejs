CHANGELOG.md
<% if (documentation) { -%>
/docs
<% } -%>
<% if (packageManager === 'yarn' && inRoot) { -%>
/.yarn/**/*
<% if (yarnNodeLinker === 'pnp') { -%>
/.pnp.*
<% } -%>
<% } -%>
<% if (workspaces) { -%>
<% for (let workspace of workspaces) { -%>
<% if (ignorePatterns) { -%>
<% for (let ignorePattern of ignorePatterns) { -%>
<%= '/' + workspace + ignorePattern %>
<% } -%>
<% } -%>
<% } -%>
<% } else { -%>
<% for (let ignorePattern of ignorePatterns) { -%>
<%= ignorePattern %>
<% } -%>
<% } -%>
