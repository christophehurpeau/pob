<% if (documentation) { -%>
/docs
<% } -%>
<% if (useYarn2 && inRoot) { -%>
/.pnp.js
/.yarn/**/*
<% } -%>
<% if (workspaces) { -%>
<% for (let workspace of workspaces) { -%>
<% if (ignorePatterns) { -%>
<% for (let ignorePattern of ignorePatterns) { -%>
<%= '/' + workspace + ignorePattern %>
<% } -%>
<% } -%>
<% } -%>
<% } else { -%>
<% for (let ignorePattern of ignorePatterns) { -%>
<%= ignorePattern %>
<% } -%>
<% } -%>
