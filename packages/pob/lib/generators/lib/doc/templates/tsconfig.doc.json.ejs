{
  "extends": "./tsconfig.json",

  "typedocOptions": {
    "entryPoints": [
      <%- entryPoints.map(entryPoint => `"${entryPoint}"`).join(', ') %>
    ],
    "exclude": [
      "src/**/*.test.ts<%= jsx ? 'x?' : '' %>",
      "src/**/__tests__/**"
    ],
    "readme": "<%= readme %>",
    "out": "docs",
    "excludePrivate": true,
    "gitRevision": "main"
  },

  "exclude": [
    "src/**/*.test.ts<%= jsx ? 'x?' : '' %>",
    "src/**/__tests__/**"
  ]
}
