{
  "extends": "./tsconfig.json",


  "compilerOptions": {
    "rootDirs": [],
    "rootDir": "src"
  },

  "typedocOptions": {
    "entryPoints": [
      <%- entryPoints.map(entryPoint => `"${entryPoint}"`).join(', ') %>
    ],
    "exclude": [
      "src/**/*.test.ts<%= jsx ? 'x?' : '' %>",
      "src/**/__tests__/**"
    ],
    "readme": "<%= readme %>",
    "out": "docs",
    "excludePrivate": true,
    "gitRevision": "master"
  },

  "exclude": [
    "src/**/*.test.ts<%= jsx ? 'x?' : '' %>",
    "src/**/__tests__/**"
  ]
}
