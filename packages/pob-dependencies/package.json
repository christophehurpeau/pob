{
  "name": "pob-dependencies",
  "version": "3.12.0",
  "description": "easier dependencies upgrade with a real package.json",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pob.git",
    "directory": "packages/pob-dependencies"
  },
  "homepage": "https://github.com/christophehurpeau/pob",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:definitions",
    "build:definitions": "tsc --lib esnext --noEmit --skipLibCheck ./lib/index.d.ts",
    "lint:eslint": "yarn --cwd ../.. eslint --report-unused-disable-directives --quiet packages/pob-dependencies",
    "preversion": "node ./scripts/update-peerdeps.js && node ./scripts/generate.d.ts.js && git add lib/index.d.ts"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {},
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "@babel/runtime": "7.14.0",
    "@pob/commitlint-config": "^2.1.0",
    "@pob/eslint-config": "43.2.0",
    "@pob/eslint-config-node": "43.2.0",
    "@pob/eslint-config-typescript": "43.2.0",
    "@pob/eslint-config-typescript-node": "43.2.0",
    "@pob/eslint-config-typescript-react": "43.2.0",
    "@pob/lerna-light": "3.24.0",
    "@pob/pretty-pkg": "2.1.0",
    "@pob/root": "^3.2.0",
    "@rollup/plugin-run": "2.0.2",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-jest": "26.6.3",
    "babel-preset-latest-node": "5.5.1",
    "babel-preset-modern-browsers": "15.0.2",
    "eslint": "7.27.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "29.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-junit-reporter": "1.1.0",
    "pob-babel": "^26.8.0",
    "pob-lcov-reporter": "4.0.1",
    "prettier": "2.2.1",
    "repository-check-dirty": "2.0.1",
    "rollup": "2.50.6",
    "semver": "7.3.5",
    "standard-version": "9.3.0",
    "typedoc": "0.20.36",
    "typescript": "4.2.4"
  }
}
