{
  "name": "pob-dependencies",
  "version": "6.30.0",
  "description": "easier dependencies upgrade with a real package.json",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/pob.git",
    "directory": "packages/pob-dependencies"
  },
  "homepage": "https://github.com/christophehurpeau/pob",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:definitions",
    "build:definitions": "tsc --lib esnext --noEmit --skipLibCheck ./lib/index.d.ts",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/pob-dependencies",
    "preversion": "node ./scripts/update-peerdeps.js && node ./scripts/generate.d.ts.js && git add lib/index.d.ts"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {},
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.18.9",
    "@pob/commitlint-config": "4.3.0",
    "@pob/eslint-config": "49.3.2",
    "@pob/eslint-config-typescript": "49.3.3",
    "@pob/eslint-config-typescript-react": "49.3.2",
    "@pob/lerna-light": "5.4.0",
    "@pob/pretty-pkg": "4.2.0",
    "@types/jest": "28.1.7",
    "alp-rollup-plugin-config": "1.0.2",
    "babel-preset-modern-browsers": "15.0.2",
    "eslint": "8.22.0",
    "jest": "28.1.3",
    "jest-junit-reporter": "1.1.0",
    "pob-babel": "34.0.0",
    "prettier": "2.7.1",
    "repository-check-dirty": "4.1.0",
    "rollup": "2.78.1",
    "semver": "7.3.7",
    "typedoc": "0.23.10",
    "typescript": "4.7.4"
  }
}
